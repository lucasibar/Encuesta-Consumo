const { DataTypes } = require('sequelize');

module.exports = (sequelize) => {
sequelize.define('DayIngredients', {
  meal:{
    type: DataTypes.STRING
  },
  amount:{
    type: DataTypes.INTIGER
  },
  unit:{
    type: DataTypes.STRING
  }
},
{
  timestamps: true,
  createdAt: true,
  updatedAt: false
  });
};
